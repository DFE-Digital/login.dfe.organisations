{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Orgs",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "settings": "${workspaceFolder}/config/login.dfe.organisations.local.json"
      },
      "program": "${workspaceFolder}/src/index.js",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Orgs Tests",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "settings": "${workspaceFolder}/config/login.dfe.organisations.local.json"
      },
      "program": "${workspaceFolder}/node_modules/.bin/jest"
    }    
  ]
}
